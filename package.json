  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "turbo": "^2.3.3",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.2.0"
}
